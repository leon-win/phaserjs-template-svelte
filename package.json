    "svelte": "4.2.18",
    "svelte-check": "3.8.4",
    "terser": "5.31.3",
    "tslib": "2.6.3",
    "typescript": "5.5.3",
    "vite": "5.3.4"
  },
  "type": "module",
  "dependencies": {
    "phaser": "3.80.1"
